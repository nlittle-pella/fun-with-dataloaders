{
    "testEnvironment": "node",
    "moduleFileExtensions": ["js", "json"],
    "transform": {
        "^.+\\.js$": "babel-jest"
    },
    "testMatch": ["<rootDir>/**/*.test.js"],
    "testTimeout": 10000,
    "maxWorkers": 1,
    "globalSetup": "./global-setup",
    "setupFilesAfterEnv": ["<rootDir>/test-setup.js"]
}
